{
	"name": "apex-lightbulbs",
	"displayName": "Apex Lightbulbs",
	"description": "Bulb actions for VSCode to speed up your development",
	"version": "1.0.0",
	"publisher": "nchursin",
	"repository": {
		"type": "git",
		"url": "https://github.com/nchursin/apex-lightbulbs.git"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:apex"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:package": "vsce package --yarn",
		"vscode:publish": "vsce publish --yarn",
		"vscode:prepublish": "yarn build:prod",
		"build:prod": "webpack --mode production",
		"precompile": "rm -rf out",
		"compile:non-ts": "bash ./scripts/nonTsCompile.sh",
		"compile": "tsc -p ./",
		"test:data": "bash ./scripts/testData.sh",
		"watch": "webpack --mode development",
		"pretest": "yarn run compile && yarn run test:data",
		"test": "node ./out/test/runTest.js"
	},
	"_moduleAliases": {
		"@assets": "assets",
		"@src": "out/src",
		"@utils": "out/src/lib/utils",
		"@labels": "out/src/labels",
		"@constants": "out/src/constants",
		"@templates": "out/src/lib/templates",
		"@languageServer": "out/src/lib/languageServer",
		"@actionProviders": "out/src/lib/actionProviders",
		"@testutils": "out/test/utils"
	},
	"devDependencies": {
		"@types/chai": "^4.2.11",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"nyc": "^15.0.0",
		"raw-loader": "^4.0.1",
		"ts-loader": "^7.0.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@types/ramda": "^0.26.44",
		"@types/shelljs": "^0.8.6",
		"@types/sinon": "^7.5.2",
		"chai": "^4.2.0",
		"es6-template-strings": "^2.0.1",
		"expand-home-dir": "^0.0.3",
		"find-java-home": "^1.1.0",
		"module-alias": "^2.2.2",
		"ramda": "^0.27.0",
		"shelljs": "^0.8.3",
		"sinon": "^9.0.1",
		"vscode-extension-telemetry": "^0.1.2",
		"vscode-languageclient": "5.1.1"
	}
}
